<aura:component implements="flexipage:availableForAllPageTypes">
        
    <!-- <aura:handler name="init" value="{!this}" action="{!c.doInit}"/> -->
    <lightning:card title="UI Namespaced Element">
        <div class="slds-p-around_small">
            <div id="outputText">
                <ui:outputText aura:id="outputText" value="This is a ui:outputText tag from the ui namespace"/>
            </div>
        </div>
    </lightning:card>


    <lightning:card title="matonis Namespaced Element">
        <div class="slds-p-around_small">
            <matonis:LockerServiceTest ></matonis:LockerServiceTest>
        </div>
    </lightning:card>

    <lightning:card title="c Namespaced Element">
        <div class="slds-p-around_small">
            <div id="cDiv">&lt;div id="cDiv"&gt;&lt;/div&gt;</div>
            <button onclick="{!c.checkForIds}">Check for ids from the C Namespace</button>
        </div>
    </lightning:card>




</aura:component>